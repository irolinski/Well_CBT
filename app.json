{
  "expo": {
    "name": "WorryFree",
    "slug": "WorryFree",
    "icon": "./assets/icons/app-icon.png",
    "version": "1.0.2",
    "orientation": "portrait",
    "scheme": "myapp",
    "newArchEnabled": true,
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#8DBED8"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.irolinski.cbtApp",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "UIRequiresFullScreen": true,
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 102,
      "adaptiveIcon": {
        "backgroundColor": "#8DBED8",
        "backgroundImage": "./assets/icons/adaptive_icon_bg.png",
        "foregroundImage": "./assets/icons/adaptive_icon_fg.png"
      },
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ],
      "package": "com.irolinski.cbtApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#8DBED8",
          "image": "./assets/icons/splash-icon.png",
          "imageWidth": 250,
          "resizemode": "cover"
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "From your contacts list, choose a friend you want to call to in times of trouble."
        }
      ],
      "expo-build-properties",
      "expo-asset",
      "expo-localization",
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          },
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-sqlite",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "86ca8438-dc82-4e22-899d-d7fec9a79fe7"
      }
    }
  }
}
